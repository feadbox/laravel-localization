{
    "name": "feadbox/laravel-localization",
    "description": "Laravel localization.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davut Kember",
            "email": "davutkmbr@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support": "^5.5|^6.20.12|^7.30.3|^8.22.1"
